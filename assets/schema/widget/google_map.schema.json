{
  "$id": "https://legytma.com.br/schema/widget/google_map.schema.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Google Map",
  "description": "",
  "$comment": "Created by Windol <windol@legytma.com.br> at 07/05/2020.\nCopyright (c) 2020 Legytma Soluções Inteligentes (https://legytma.com.br). All rights reserved.",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://legytma.com.br/schema/widget.schema.json"
    }
  ],
  "properties": {
    "type": {
      "$ref": "https://legytma.com.br/schema/widget.schema.json#properties/type",
      "const": "GoogleMap"
    },
    "initialCameraPosition": {"$ref": "https://legytma.com.br/schema/camera_position.schema.json"},
    "myLocationEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": false
    },
    "mapType": {
      "$ref": "https://legytma.com.br/schema/map_type.schema.json",
      "default": "normal"
    },
    "compassEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    },
    "onTap": {"$ref": "https://legytma.com.br/schema/argument_callback_lat_lng.schema.json"},
    "padding": {
      "$ref": "https://legytma.com.br/schema/edge_insets.schema.json",
      "default": {
        "type": "all",
        "all": 0.0
      }
    },
    "onLongPress": {"$ref": "https://legytma.com.br/schema/argument_callback_lat_lng.schema.json"},
    "buildingsEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    },
    "cameraTargetBounds": {"$ref": "https://legytma.com.br/schema/camera_target_bounds.schema.json"},
    "circles": {"$ref": "https://legytma.com.br/schema/set_circle.schema.json"},
    "gestureRecognizers": {"$ref": "https://legytma.com.br/schema/set_factory_one_sequence_gesture_recognizer.schema.json"},
    "indoorViewEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": false
    },
    "mapToolbarEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    },
    "markers": {"$ref": "https://legytma.com.br/schema/set_marker.schema.json"},
    "minMaxZoomPreference": {
      "$ref": "https://legytma.com.br/schema/min_max_zoom_preference.schema.json",
      "default": "unbounded"
    },
    "myLocationButtonEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    },
    "onCameraIdle": {"$ref": "https://legytma.com.br/schema/void_callback.schema.json"},
    "onCameraMove": {"$ref": "https://legytma.com.br/schema/camera_position_callback.schema.json"},
    "onCameraMoveStarted": {"$ref": "https://legytma.com.br/schema/void_callback.schema.json"},
    "onMapCreated": {"$ref": "https://legytma.com.br/schema/map_created_callback.schema.json"},
    "polygons": {"$ref": "https://legytma.com.br/schema/set_polygon.schema.json"},
    "polylines": {"$ref": "https://legytma.com.br/schema/set_polyline.schema.json"},
    "rotateGesturesEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    },
    "scrollGesturesEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    },
    "tiltGesturesEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    },
    "trafficEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": false
    },
    "zoomControlsEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    },
    "zoomGesturesEnabled": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": true
    }
  },
  "required": ["initialCameraPosition"]
}

