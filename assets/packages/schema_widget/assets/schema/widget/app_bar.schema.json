{
  "$id": "https://schema.legytma.com.br/3.0.0/schema/widget/app_bar.schema.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "App Bar",
  "description": "",
  "$comment": "Created by Windol <windol@legytma.com.br> at 01/05/2020.\nCopyright (c) 2020 Legytma Soluções Inteligentes (https://legytma.com.br). All rights reserved.",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://schema.legytma.com.br/3.0.0/schema/widget.schema.json"
    }
  ],
  "properties": {
    "type": {
      "$ref": "https://schema.legytma.com.br/3.0.0/schema/widget.schema.json#/definitions/type",
      "const": "AppBar"
    },
    "title": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/widget.schema.json"},
    "primary": {
      "$ref": "https://schema.legytma.com.br/3.0.0/schema/bool.schema.json",
      "default": true
    },
    "actions": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/list_widget.schema.json"},
    "actionsIconTheme": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/icon_theme_data.schema.json"},
    "automaticallyImplyLeading": {
      "$ref": "https://schema.legytma.com.br/3.0.0/schema/bool.schema.json",
      "default": true
    },
    "backgroundColor": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/icon_theme_data.schema.json"},
    "bottom": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/preferred_size_widget.schema.json"},
    "bottomOpacity": {
      "$ref": "https://schema.legytma.com.br/3.0.0/schema/double.schema.json",
      "default": 1.0
    },
    "brightness": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/brightness.schema.json"},
    "centerTitle": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/bool.schema.json"},
    "elevation": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/double.schema.json"},
    "flexibleSpace": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/widget.schema.json"},
    "iconTheme": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/icon_theme_data.schema.json"},
    "leading": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/widget.schema.json"},
    "shape": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/shape_border.schema.json"},
    "textTheme": {"$ref": "https://schema.legytma.com.br/3.0.0/schema/text_theme.schema.json"},
    "titleSpacing": {
      "$ref": "https://schema.legytma.com.br/3.0.0/schema/double.schema.json",
      "default": 16.0
    },
    "toolbarOpacity": {
      "$ref": "https://schema.legytma.com.br/3.0.0/schema/double.schema.json",
      "default": 1.0
    }
  }
}

