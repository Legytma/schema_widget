{
  "$id": "https://legytma.com.br/schema/widget/image_file.schema.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Image File",
  "description": "",
  "$comment": "Created by Windol <windol@legytma.com.br> at 12/05/2020.\nCopyright (c) 2020 Legytma Soluções Inteligentes (https://legytma.com.br). All rights reserved.",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://legytma.com.br/schema/widget/image.schema.json"
    }
  ],
  "properties": {
    "imageType": {
      "$ref": "https://legytma.com.br/schema/widget/image.schema.json#properties/imageType",
      "const": "file"
    },
    "file": {
      "title": "File",
      "description": "",
      "$comment": "",
      "type": "string"
    },
    "cacheHeight": {"$ref": "https://legytma.com.br/schema/int.schema.json"},
    "cacheWidth": {"$ref": "https://legytma.com.br/schema/int.schema.json"},
    "scale": {
      "$ref": "https://legytma.com.br/schema/double.schema.json",
      "default": 1.0
    }
  },
  "required": ["imageType", "file"]
}

