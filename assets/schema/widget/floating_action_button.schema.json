{
  "$id": "https://legytma.com.br/schema/widget/floating_action_button.schema.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Floating Action Button",
  "description": "",
  "$comment": "Created by Windol <windol@legytma.com.br> at 06/05/2020.\nCopyright (c) 2020 Legytma Soluções Inteligentes (https://legytma.com.br). All rights reserved.",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://legytma.com.br/schema/widget.schema.json"
    }
  ],
  "properties": {
    "type": {
      "$ref": "https://legytma.com.br/schema/widget.schema.json#properties/type",
      "const": "FloatingActionButton"
    },
    "onPressed": {"$ref": "https://legytma.com.br/schema/void_callback.schema.json"},
    "autofocus": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": false
    },
    "backgroundColor": {"$ref": "https://legytma.com.br/schema/color.schema.json"},
    "child": {"$ref": "https://legytma.com.br/schema/widget.schema.json"},
    "clipBehavior": {
      "$ref": "https://legytma.com.br/schema/clip.schema.json",
      "default": "none"
    },
    "disabledElevation": {"$ref": "https://legytma.com.br/schema/double.schema.json"},
    "elevation": {"$ref": "https://legytma.com.br/schema/double.schema.json"},
    "focusColor": {"$ref": "https://legytma.com.br/schema/color.schema.json"},
    "focusElevation": {"$ref": "https://legytma.com.br/schema/double.schema.json"},
    "focusNode": {"$ref": "https://legytma.com.br/schema/focus_node.schema.json"},
    "foregroundColor": {"$ref": "https://legytma.com.br/schema/color.schema.json"},
    "highlightElevation": {"$ref": "https://legytma.com.br/schema/double.schema.json"},
    "hoverColor": {"$ref": "https://legytma.com.br/schema/color.schema.json"},
    "hoverElevation": {"$ref": "https://legytma.com.br/schema/double.schema.json"},
    "isExtended": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": false
    },
    "materialTapTargetSize": {"$ref": "https://legytma.com.br/schema/material_tap_target_size.schema.json"},
    "mini": {
      "$ref": "https://legytma.com.br/schema/bool.schema.json",
      "default": false
    },
    "shape": {"$ref": "https://legytma.com.br/schema/shape_border.schema.json"},
    "splashColor": {"$ref": "https://legytma.com.br/schema/color.schema.json"},
    "tooltip": {
      "title": "Tooltip",
      "description": "",
      "$comment": "",
      "type": "string"
    }
  }
